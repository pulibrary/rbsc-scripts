    <!-- PUBLICATIONSTMT/DATE adds encodinganalog and normalizes date-->
    <xsl:template match="ead:publicationstmt/ead:date">
        <xsl:copy>
            <xsl:copy-of select="@*[not(name()='label')]"/>
            <xsl:attribute name="encodinganalog">dc:date</xsl:attribute>
            <xsl:attribute name="normal">
                <xsl:analyze-string select="string(.)" regex="\d{{4}}">
                    <xsl:matching-substring>
                        <xsl:value-of select="normalize-space(current())"/>
                    </xsl:matching-substring>
                </xsl:analyze-string>
            </xsl:attribute>
            <xsl:if test="not(matches(., 'published in', 'i'))">
            <xsl:text>Published in </xsl:text>
            <xsl:apply-templates/>
            <xsl:text>.</xsl:text>
            </xsl:if>
            <xsl:if test="matches(., 'published in', 'i')">
                <xsl:apply-templates/>
            </xsl:if>
        </xsl:copy>
    </xsl:template>

    <!-- PHYSDESC this template creates one physdesc for all physdesc's children -->
    <xsl:template match="ead:did[ead:physdesc[ancestor::ead:dsc]]">
        <xsl:copy>
            <xsl:copy-of select="@*"/>       
            <xsl:apply-templates select="ead:*[not(self::ead:physdesc)]"/>
            <physdesc>
        <xsl:for-each select=".//ead:extent">
            <xsl:apply-templates select="."/>
            <xsl:text> </xsl:text>
        </xsl:for-each>
        <xsl:for-each select=".//ead:dimensions">
            <xsl:apply-templates select="."/>
            <xsl:text> </xsl:text>
        </xsl:for-each>
        <xsl:for-each select=".//ead:physfacet">
            <xsl:apply-templates select="."/>
            <xsl:text> </xsl:text>
        </xsl:for-each>
        <xsl:for-each select="./ead:physdesc/text()">
            <xsl:apply-templates select="."/>
            <xsl:text> </xsl:text>
        </xsl:for-each>
            </physdesc>

        </xsl:copy>
    </xsl:template>


