<?xml version="1.0" encoding="UTF-8"?>
<xsl:stylesheet xmlns="urn:isbn:1-931666-22-9" xmlns:xsl="http://www.w3.org/1999/XSL/Transform"
    xmlns:fn="http://www.w3.org/2005/xpath-functions" xmlns:xs="http://www.w3.org/2001/XMLSchema"
    xmlns:xd="http://www.oxygenxml.com/ns/doc/xsl" xmlns:ead="urn:isbn:1-931666-22-9"
    xmlns:xlink="http://www.w3.org/1999/xlink" xmlns:ns2="http://www.w3.org/1999/xlink"
    version="2.0" exclude-result-prefixes="#all">

    <xsl:output method="xml" indent="yes" exclude-result-prefixes="#all"/>
    <xsl:strip-space elements="p"/>

    <!--  EAD:EAD: This template adds the @audience and the ns's ead and xlink to the root element-->
    <xsl:template match="/*">
        <xsl:copy>
            <xsl:attribute name="audience" select="'external'"/>
            <xsl:namespace name="ead" select="'urn:isbn:1-931666-22-9'"/>
            <xsl:namespace name="xlink" select="'http://www.w3.org/1999/xlink'"/>
            <xsl:apply-templates select="@* | node()"/>
        </xsl:copy>
    </xsl:template>

    <!-- This template copies everything unless matched below-->
    <xsl:template match="@*|node()">
        <xsl:copy>
            <xsl:apply-templates select="@*[not(name()='label')]|node()"/>
        </xsl:copy>
    </xsl:template>

    <!-- Don't copy the following elements: -->
    <xsl:template match="ead:head"/>
    <xsl:template match="ead:titleproper/ead:num"/>
    <xsl:template match="ead:archdesc[ead:dao and ..//ead:creation[matches(., 'Toolkit', 'i')]]/ead:dao"/>
    <xsl:template match="ead:archdesc/ead:arrangement/ead:list | ead:archdesc/ead:descgrp/ead:arrangement/ead:list"/>
    <xsl:template match="ead:arrangement[not(ead:address | ead:arrangement | ead:blockquote | ead:chronlist | ead:note | ead:p | ead:table)]"/>
    <xsl:template match="ead:langmaterial[count(ead:language)=1 and ead:language[@langcode='mul']]"/>

    <!-- EADID: This template adds @encodinganalog, @urn, and @url  -->
    <xsl:template match="ead:eadid">
        <xsl:variable name="url" as="xs:string*">
            <xsl:value-of select="@url"/>
        </xsl:variable>
        <xsl:variable name="urn" as="xs:string*" select="tokenize($url, '.edu/')"/>
        <xsl:copy>
            <xsl:copy-of select="@*"/>
            <xsl:attribute name="encodinganalog">dc:identifier</xsl:attribute>
            <xsl:attribute name="urn">
                <xsl:value-of select="$urn[position()=last()]"/>
            </xsl:attribute>
            <xsl:attribute name="url">
                <xsl:value-of select="@url"/>
            </xsl:attribute>
            <xsl:apply-templates/>
        </xsl:copy>
    </xsl:template>

    <!-- LANGUSAGE: inserts boilerplate -->
    <xsl:template match="ead:profiledesc/ead:langusage">
        <langusage>
            <xsl:text>Finding aid written in </xsl:text>
            <language encodinganalog="dc:language" langcode="eng">
                <xsl:text>English</xsl:text>
            </language>. </langusage>
    </xsl:template>

    <!-- DESCRULES: insert boilerplate -->
    <xsl:template match="ead:profiledesc/ead:descrules">
        <descrules>Finding aid content adheres to that prescribed by <emph render="italic">
                <expan abbr="dacs" altrender="MARC">Describing Archives: A Content Standard</expan>.
            </emph>
        </descrules>
    </xsl:template>

    <!-- TITLEPROPER: adds @encodinganalog-->
    <xsl:template match="ead:titleproper">
        <xsl:copy>
            <xsl:copy-of select="@*[not(name()='label')]"/>
            <xsl:attribute name="encodinganalog">dc:title</xsl:attribute>
            <xsl:apply-templates/>
        </xsl:copy>
    </xsl:template>

    <!-- TITLEPROPER/DATE adds encodinganalog -->
    <xsl:template match="ead:titleproper/ead:date">
        <xsl:copy>
            <xsl:copy-of select="@*[not(name()='label')]"/>
            <xsl:attribute name="encodinganalog">dc:date</xsl:attribute>
            <xsl:apply-templates/>
        </xsl:copy>
    </xsl:template>

    <!-- TITLESTMT/AUTHOR adds punctuation -->
    <xsl:template match="ead:titlestmt/ead:author">
        <xsl:copy>
            <xsl:copy-of select="@*[not(name()='label')]"/>
            <xsl:apply-templates/>
            <xsl:text>.</xsl:text>
        </xsl:copy>
    </xsl:template>

    <!-- ARCHDESC: This template adds two attributes -->
    <xsl:template match="ead:archdesc">
        <xsl:copy copy-namespaces="no">
            <xsl:copy-of select="@*[not(name()='label')]"/>
            <xsl:attribute name="relatedencoding">marc21</xsl:attribute>
            <xsl:attribute name="type">findingaid</xsl:attribute>
            <xsl:apply-templates select="ead:did|ead:bioghist|ead:descgrp|ead:dao"/>
            <xsl:if test="self::ead:archdesc[not(ead:descgrp)]">
            <!-- ead:scopecontent|ead:arrangement: dacs3 and attributes -->
            <xsl:if test="ead:scopecontent|ead:arrangement">
                <descgrp id="dacs3">
                    <xsl:if test="ead:scopecontent">
                        <scopecontent encodinganalog="520$a">
                            <xsl:apply-templates select="ead:scopecontent/*"/>
                        </scopecontent>
                    </xsl:if>
                    <xsl:if test="ead:arrangement[ead:address | ead:arrangement | ead:blockquote | ead:chronlist | ead:note | ead:p | ead:table]">
                        <arrangement encodinganalog="351$a">
                     <!--<xsl:choose>
                                <xsl:when test="ead:arrangement/ead:list[@type[.='ordered']]">
                                    <xsl:apply-templates
                                        select="ead:arrangement/*[not(.[@type[.='ordered']])]"/>
                                    <list type="simple">
                                        <xsl:for-each select="//ead:arrangement//ead:ref">
                                            <item>
                                                <xsl:variable name="refposition" select="position()"
                                                  as="xs:integer"/>
                                                <ref>
                                                  <xsl:copy-of select="@altrender"/>
                                                  <xsl:attribute name="target">
                                                  <xsl:value-of
                                                  select="(//ead:c[@level='series'] | //ead:c[@level='subseries'])[position()=$refposition]/@id"
                                                  />
                                                  </xsl:attribute>
                                                  <xsl:apply-templates select="./*"/>
                                                </ref>
                                            </item>
                                        </xsl:for-each>
                                    </list>
                                </xsl:when>
                                <xsl:otherwise>-->
                                    <xsl:apply-templates select="ead:arrangement/*"/>
<!--                                </xsl:otherwise>
                            </xsl:choose>-->
                        </arrangement>
                    </xsl:if>
                </descgrp>
            </xsl:if>
            <!-- ead:accessrestrict | ead:userestrict  | ead:phystech  | ead:otherfindaid: dacs4 and attributes -->
            <xsl:if
        test="ead:accessrestrict | ead:userestrict  | ead:phystech  | ead:otherfindaid">
        <descgrp id="dacs4">
            <xsl:choose>
                <xsl:when test="ead:accessrestrict ">
                    <xsl:choose>
                        <xsl:when
                            test="ead:accessrestrict/ead:head[matches(., 'remote\s*storage', 'i') or matches(., '^\s*access\s*$', 'i') or matches(., '^\s*access\s*restrictions\s*$', 'i') or matches(., '^\s*digital\s*access\s*$', 'i')]">
                            <xsl:if
                                test="ead:accessrestrict[ead:head[matches(., 'remote\s*storage', 'i')]]">
                                <accessrestrict encodinganalog="506$a">
                                    <xsl:attribute name="type">remotestorage</xsl:attribute>
                                    <xsl:apply-templates
                                        select="ead:accessrestrict[ead:head[matches(., 'remote\s*storage', 'i')]]/*"
                                    />
                                </accessrestrict>
                            </xsl:if>
                            <xsl:if
                                test="ead:accessrestrict[ead:head[matches(., '^\s*access\s*$', 'i')]]">
                                <accessrestrict encodinganalog="506$a">
                                    <xsl:attribute name="type">open</xsl:attribute>
                                    <xsl:apply-templates
                                        select="ead:accessrestrict[ead:head[matches(., '^\s*access\s*$', 'i')]]/*"
                                    />
                                </accessrestrict>
                            </xsl:if>
                            <xsl:if
                                test="ead:accessrestrict[ead:head[matches(., '^\s*access\s*restrictions\s*$', 'i')]]">
                                <accessrestrict encodinganalog="506$a">
                                    <xsl:attribute name="type">closed</xsl:attribute>
                                    <xsl:apply-templates
                                        select="ead:accessrestrict[ead:head[matches(., '^\s*access\s*restrictions\s*$', 'i')]]/*"
                                    />
                                </accessrestrict>
                            </xsl:if>
                            <xsl:if
                                test="ead:accessrestrict[ead:head[matches(., '^\s*digital\s*access\s*$', 'i')]]">
                                <accessrestrict encodinganalog="506$a">
                                    <xsl:attribute name="type">digital</xsl:attribute>
                                    <xsl:apply-templates
                                        select="ead:accessrestrict[ead:head[matches(., '^\s*digital\s*access\s*$', 'i')]]/*"
                                    />
                                </accessrestrict>
                            </xsl:if>
                        </xsl:when>
                        <xsl:otherwise>
                            <xsl:message>Warning: accessrestrict/head does not match one of the
                                following: Access / Remote Storage / Digital Access / Access
                                Restrictions</xsl:message>
                        </xsl:otherwise>
                    </xsl:choose>
                </xsl:when>
                <xsl:otherwise>
                    <accessrestrict encodinganalog="506$a" type="open">
                        <p>Collection is open for research use.</p>
                    </accessrestrict>
                </xsl:otherwise>
            </xsl:choose>
            <xsl:choose>
                <xsl:when test="ead:userestrict">
                    <userestrict encodinganalog="540$a">
                        <xsl:apply-templates select="ead:userestrict/*"/>
                    </userestrict>
                </xsl:when>
                <xsl:otherwise>
                    <userestrict encodinganalog="540$a">
                        <p>Photocopies may be made for research purposes. Researchers are
                            responsible for determining any copyright questions.</p>
                    </userestrict>
                </xsl:otherwise>
            </xsl:choose>
            <xsl:if test="ead:phystech">
                <phystech encodinganalog="538$a">
                    <xsl:apply-templates select="ead:phystech/*"/>
                </phystech>
            </xsl:if>
            <xsl:if test="ead:otherfindaid">
                <otherfindaid encodinganalog="555$u">
                    <xsl:apply-templates select="ead:otherfindaid/*"/>
                </otherfindaid>
            </xsl:if>
        </descgrp>
    </xsl:if>

            <!--  ead:custodhist | ead:acqinfo | ead:appraisal | ead:accruals: dacs5 and attributes-->
            <xsl:if test="ead:custodhist | ead:acqinfo | ead:appraisal | ead:accruals">
        <descgrp id="dacs5">
            <xsl:if test="ead:custodhist">
                <custodhist encodinganalog="561$a">
                    <xsl:apply-templates select="ead:custodhist/*"/>
                </custodhist>
            </xsl:if>
            <xsl:if test="ead:acqinfo">
                <acqinfo encodinganalog="541$a">
                    <xsl:apply-templates select="ead:acqinfo/*"/>
                </acqinfo>
            </xsl:if>
            <xsl:if test="ead:appraisal">
                <appraisal encodinganalog="583$a">
                    <xsl:apply-templates select="ead:appraisal/*"/>
                </appraisal>
            </xsl:if>
            <xsl:if test="ead:accruals">
                <accruals encodinganalog="584$a">
                    <xsl:apply-templates select="ead:accruals/*"/>
                </accruals>
            </xsl:if>
        </descgrp>
    </xsl:if>

            <!-- ead:originalsloc | ead:altformavail | ead:relatedmaterial | 
        ead:bibliography/ead:head[matches(string(.), 'Publications Citing These Papers', 'i')]: dacs6 and attributes -->
             <xsl:if test="ead:originalsloc | ead:altformavail | ead:relatedmaterial | 
        ead:bibliography/ead:head[matches(string(.), 'Publications Citing These Papers', 'i')]">
        <descgrp id="dacs6">
            <xsl:if test="ead:originalsloc">
                <originalsloc encodinganalog="535$a">
                    <xsl:apply-templates select="ead:originalsloc/*"/>
                </originalsloc>
            </xsl:if>
            <xsl:if test="ead:altformavail">
                <altformavail encodinganalog="530$a">
                    <xsl:apply-templates select="ead:altformavail/*"/>
                </altformavail>
            </xsl:if>
            <xsl:if test="ead:relatedmaterial">
                <relatedmaterial encodinganalog="544$a">
                    <xsl:apply-templates select="ead:relatedmaterial/*"/>
                </relatedmaterial>
            </xsl:if>
            <xsl:if
                test="ead:bibliography/ead:head[matches(string(.), 'Publications Citing These Papers', 'i')]">
                <bibliography encodinganalog="581$a">
                    <xsl:apply-templates
                        select="ead:bibliography[./matches(string(.), 'Publications Citing These Papers', 'i')]/*"
                    />
                </bibliography>
            </xsl:if>
        </descgrp>
    </xsl:if>

            <!-- ead:note | ead:odd | ead:processinfo | ead:prefercite | 
        ead:bibliography/ead:head[matches(string(.), 'Works Cited', 'i')]: dacs7 and attributes -->
            <xsl:if test="ead:note | ead:odd | ead:processinfo | ead:prefercite | 
        ead:bibliography/ead:head[matches(string(.), 'Works Cited', 'i')]">
        <descgrp id="dacs7">
            <xsl:if test="ead:note">
                <note>
                    <xsl:apply-templates select="ead:note/*"/> /> </note>
            </xsl:if>
            <xsl:if test="ead:odd">
                <odd>
                    <xsl:apply-templates select="ead:odd/*"/>
                </odd>
            </xsl:if>
            <xsl:if test="ead:processinfo/ead:head[matches(string(.), 'Conservation', 'i')]">
                <processinfo id="conservation" encodinganalog="583$a">
                    <xsl:apply-templates
                        select="ead:processinfo[./matches(string(.), 'Conservation', 'i')]/*"
                    />
                </processinfo>
            </xsl:if>
            <xsl:if test="ead:prefercite">
                <prefercite encodinganalog="524$a">
                    <xsl:apply-templates select="ead:prefercite/*"/>
                </prefercite>
            </xsl:if>
            <xsl:if test="ead:bibliography/ead:head[matches(string(.), 'Works Cited', 'i')]">
                <bibliography encodinganalog="581$a">
                    <xsl:apply-templates
                        select="ead:bibliography[./matches(string(.), 'Works Cited', 'i')]/*"
                    />
                </bibliography>
            </xsl:if>
            <xsl:if
                test="ead:processinfo/ead:head[matches(string(.), 'Processing Information', 'i')]">
                <processinfo id="processing" encodinganalog="583$a">
                    <xsl:apply-templates
                        select="ead:processinfo[./matches(string(.), 'Processing Information', 'i')]/*"
                    />
                </processinfo>
            </xsl:if>
        </descgrp>
    </xsl:if>
            </xsl:if>
            <!-- ARCHDESC LACKING DAO: put in logo -->
            <xsl:if test="self::ead:archdesc[not(//ead:dao)]">
                <xsl:if test="matches(//ead:did/ead:unitid, '^C')">
                    <dao xlink:title="Princeton University Manuscripts Division"
                        xlink:href="bioghist-images/msslogo.jpg"/>
                </xsl:if>
                <xsl:if test="matches(//ead:did/ead:unitid, '^GC')">
                    <dao xlink:title="Princeton University Manuscripts Division"
                        xlink:href="bioghist-images/msslogo.jpg"/>
                </xsl:if>
                <xsl:if test="matches(//ead:did/ead:unitid, '^RCP')">
                    <dao xlink:title="Princeton University Manuscripts Division"
                        xlink:href="bioghist-images/msslogo.jpg"/>
                </xsl:if>
                <xsl:if test="matches(//ead:did/ead:unitid, '^RTC')">
                    <dao xlink:title="Princeton University Manuscripts Division"
                        xlink:href="bioghist-images/msslogo.jpg"/>
                </xsl:if>
                <xsl:if test="matches(//ead:did/ead:unitid, '^TC')">
                    <dao xlink:title="Princeton University Manuscripts Division"
                        xlink:href="bioghist-images/msslogo.jpg"/>
                </xsl:if>
                <xsl:if test="matches(//ead:did/ead:unitid, '^WC')">
                    <xsl:if
                        test="matches(//ead:publicationstmt/ead:address/ead:addressline, 'Manuscripts Division', 'i')">
                        <dao xlink:title="Princeton University Manuscripts Division"
                            xlink:href="bioghist-images/msslogo.jpg"/>
                    </xsl:if>
                    <xsl:if
                        test="matches(//ead:publicationstmt/ead:address/ead:addressline, 'Mudd', 'i')">
                        <dao xlink:title="Princeton University Public Policy Collections"
                            xlink:href="bioghist-images/shieldlogo.jpg"/>
                    </xsl:if>
                </xsl:if>
                <xsl:if test="matches(//ead:did/ead:unitid, '^LAE')">
                    <dao xlink:title="Princeton University Latin American Ephemera Collections"
                        xlink:href="bioghist-images/laelogo.jpg"/>
                </xsl:if>
                <xsl:if test="matches(//ead:did/ead:unitid, '^ENG')">
                    <dao xlink:title="Princeton University Engineering Collections"
                        xlink:href="bioghist-images/shieldlogo.jpg"/>
                </xsl:if>
                <xsl:if test="matches(//ead:did/ead:unitid, '^PPL')">
                    <dao xlink:title="Princeton University Engineering Collections"
                        xlink:href="bioghist-images/shieldlogo.jpg"/>
                </xsl:if>
                <xsl:if test="matches(//ead:did/ead:unitid, '^ST')">
                    <dao xlink:title="Princeton University Engineering Collections"
                        xlink:href="bioghist-images/shieldlogo.jpg"/>
                </xsl:if>
                <xsl:if test="matches(//ead:did/ead:unitid, '^MC')">
                    <dao xlink:title="Princeton University Public Policy Collections"
                        xlink:href="bioghist-images/shieldlogo.jpg"/>
                </xsl:if>
                <xsl:if test="matches(//ead:did/ead:unitid, '^AC')">
                    <dao xlink:title="Princeton University Archives"
                        xlink:href="bioghist-images/shieldlogo.jpg"/>
                </xsl:if>
            </xsl:if>
            <!-- DAO coming from AT -->
            <xsl:if test="self::ead:archdesc[ead:dao and ..//ead:creation[matches(., 'Toolkit', 'i')]]">
        <dao>
            <xsl:attribute name="xlink:title">
                <xsl:value-of select="normalize-space(//ead:archdesc/ead:dao/ead:daodesc/ead:p)"/>
            </xsl:attribute>
            <xsl:attribute name="xlink:href">
                <xsl:value-of select="//ead:archdesc/ead:dao/@xlink:href|//ead:archdesc/ead:dao/@ns2:href"/>
            </xsl:attribute>
        </dao>
            </xsl:if>
            <!-- DAO coming from bioghist -->
            <xsl:if test="self::ead:archdesc[ead:bioghist/ead:dao]">
                    <xsl:apply-templates select="ead:bioghist/ead:dao"></xsl:apply-templates>
                
            </xsl:if>
            <xsl:apply-templates select="ead:controlaccess|ead:dsc"/>
        </xsl:copy>
    </xsl:template>

    <!-- ARCHDESC/DID/UNITID: This template adds attributes to unitid -->
    <xsl:template match="ead:archdesc/ead:did/ead:unitid">
        <xsl:copy>
            <xsl:copy-of select="@*[not(name()='label')]"/>
            <xsl:attribute name="encodinganalog">084$a</xsl:attribute>
            <xsl:attribute name="countrycode">US</xsl:attribute>
            <xsl:attribute name="repositorycode">US-NjP</xsl:attribute>
            <xsl:attribute name="type">collection</xsl:attribute>
            <xsl:apply-templates/>
        </xsl:copy>
    </xsl:template>

    <!--ARCHDESC/DID/UNITTITLE: Inserts @encodinganalog  -->
    <xsl:template match="ead:archdesc/ead:did/ead:unittitle">
        <xsl:copy>
            <xsl:copy-of select="@*"/>
            <xsl:attribute name="encodinganalog">245$a</xsl:attribute>
            <xsl:apply-templates/>
        </xsl:copy>
    </xsl:template>

    <!-- ARCHDESC/DID/ORIGINATION: This template adds attributes to origination -->
    <xsl:template match="ead:archdesc/ead:did/ead:origination">
        <xsl:copy>
            <xsl:copy-of select="@*[not(name()='label')]"/>
            <xsl:if test="ead:persname[not(.='') and not(@role='Contributor (ctb)')]">
                <xsl:attribute name="encodinganalog">600</xsl:attribute>
            </xsl:if>
            <xsl:if test="ead:corpname[not(.='') and not(@role='Contributor (ctb)')]">
                <xsl:attribute name="encodinganalog">610</xsl:attribute>
            </xsl:if>
            <!-- LAE: put in encodinganalog, altrender -->
            <xsl:if test="matches(//ead:archdesc/ead:did/ead:unitid, '^LAE')">
                <xsl:attribute name="encodinganalog">110</xsl:attribute>
                <xsl:attribute name="altrender">
                    <xsl:value-of select="//ead:archdesc/ead:controlaccess/ead:geogname[@source='LAE']"></xsl:value-of>
                </xsl:attribute>
            </xsl:if>
            <xsl:apply-templates/>
        </xsl:copy>
    </xsl:template>

    <!-- ARCHDESC/DID/ORIGINATION/*: @role -->
    <xsl:template match="ead:archdesc/ead:did/ead:origination/*">
        <xsl:copy>
            <xsl:copy-of select="@*[not(name()='label')]"/>
            <xsl:if test="matches(../@label, 'creator', 'i')">
                <xsl:attribute name="role">cre</xsl:attribute>
            </xsl:if>
            <xsl:if test="matches(../@label, 'collector', 'i')">
                <xsl:attribute name="role">col</xsl:attribute>
            </xsl:if>
            <xsl:apply-templates/>
        </xsl:copy>
    </xsl:template>

    <!-- ARCHDESC/DID/EXTENT: This template adds an attribute to extent -->
    <xsl:template match="ead:archdesc/ead:did/ead:physdesc/ead:extent">
        <xsl:copy>
            <xsl:copy-of select="@*[not(name()='label')]"/>
            <xsl:attribute name="encodinganalog">300$a</xsl:attribute>
            <xsl:apply-templates/>
        </xsl:copy>
    </xsl:template>

    <!-- ARCHDESC/DID/LANGMATERIAL: This template adds attributes to langmaterial-->
    <xsl:template match="ead:archdesc/ead:did/ead:langmaterial[ead:language[not(@langcode='mul')]]">
        <xsl:copy>
            <xsl:copy-of select="@*[not(name()='label')]"/>
            <xsl:attribute name="encodinganalog">546$a</xsl:attribute>
            <xsl:for-each select="ead:language[not(@langcode='mul')]">
                <xsl:copy>
                    <xsl:copy-of select="@*[not(name()='label')]"/>
                    <xsl:attribute name="encodinganalog">041$a</xsl:attribute>
                    <xsl:apply-templates/>
                </xsl:copy>
            </xsl:for-each>
        </xsl:copy>
    </xsl:template>

    <!-- ARCHDESC/DID/REPOSITORY: This template adds repository address and attributes-->
    <xsl:template match="ead:archdesc/ead:did/ead:repository">
        <xsl:copy>
            <xsl:copy-of select="@*[not(name()='label')]"/>
            <xsl:if test="matches(//ead:archdesc/ead:did/ead:unitid, '^C')">
                <xsl:attribute name="id">mss</xsl:attribute>
            </xsl:if>
            <xsl:if test="matches(//ead:archdesc/ead:did/ead:unitid, '^GC')">
                <xsl:attribute name="id">mss</xsl:attribute>
            </xsl:if>
            <xsl:if test="matches(//ead:archdesc/ead:did/ead:unitid, '^RCP')">
                <xsl:attribute name="id">mss</xsl:attribute>
            </xsl:if>
            <xsl:if test="matches(//ead:archdesc/ead:did/ead:unitid, '^RTC')">
                <xsl:attribute name="id">mss</xsl:attribute>
            </xsl:if>
            <xsl:if test="matches(//ead:archdesc/ead:did/ead:unitid, '^TC')">
                <xsl:attribute name="id">mss</xsl:attribute>
            </xsl:if>
            <xsl:if test="matches(//ead:archdesc/ead:did/ead:unitid, '^WC')">
                <xsl:if
                    test="matches(//ead:publicationstmt/ead:address/ead:addressline, 'Manuscripts Division', 'i')">
                    <xsl:attribute name="id">mss</xsl:attribute>
                </xsl:if>
                <xsl:if
                    test="matches(//ead:publicationstmt/ead:address/ead:addressline, 'Mudd', 'i')">
                    <xsl:attribute name="id">publicpolicy</xsl:attribute>
                </xsl:if>
            </xsl:if>
            <xsl:if test="matches(//ead:archdesc/ead:did/ead:unitid, '^LAE')">
                <xsl:attribute name="id">lae</xsl:attribute>
            </xsl:if>
            <xsl:if test="matches(//ead:archdesc/ead:did/ead:unitid, '^ENG')">
                <xsl:attribute name="id">eng</xsl:attribute>
            </xsl:if>
            <xsl:if test="matches(//ead:archdesc/ead:did/ead:unitid, '^PPL')">
                <xsl:attribute name="id">eng</xsl:attribute>
