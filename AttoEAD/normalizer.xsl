            <xsl:choose>
                <xsl:when
                    test="matches(., '^mss$') or matches(., '^rcpxr$') or matches(., '^flm$') or matches(., '^flmp$') or matches(., '^rcppf$') 
                    or matches(., '^ex$') or matches(., '^gax$') or matches(., '^wa$') or matches(., '^rcpph$') or matches(., '^rcpxg$') 
                    or matches(., '^rcpxm$') or matches(., '^anxb$') or matches(., '^mudd$') or matches(., '^hsvc$') or matches(., '^hsvg$') 
                    or matches(., '^hsvr$') or matches(., '^hsvm$') or matches(., '^ctsn$') or matches(., '^thx$')">
