                                    </xsl:otherwise>
                                </xsl:choose></xsl:if>
                            <xsl:if test="matches(regex-group(6), '^May$', 'i')">-05-<xsl:choose>
                                    <xsl:when test="matches(regex-group(8),'^\d{1}$')"
                                            >0<xsl:value-of select="regex-group(8)"/>
                                    </xsl:when>
                                    <xsl:otherwise><xsl:value-of select="regex-group(8)"/>
                                    </xsl:otherwise>
                                </xsl:choose></xsl:if>
                            <xsl:if test="matches(regex-group(6), '^June$', 'i')">-06-<xsl:choose>
                                    <xsl:when test="matches(regex-group(8),'^\d{1}$')"
                                            >0<xsl:value-of select="regex-group(8)"/>
                                    </xsl:when>
                                    <xsl:otherwise><xsl:value-of select="regex-group(8)"/>
