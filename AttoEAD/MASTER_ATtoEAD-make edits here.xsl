    <!-- DSC: Add attribute, head element -->
    <xsl:template match="ead:dsc" mode="hl-did">
        <xsl:if test="not(.='')">
            <xsl:copy>
                <xsl:copy-of select="@*"/>
                <xsl:attribute name="type">combined</xsl:attribute>
                <head>Contents List</head>
                <xsl:apply-templates mode="hl-did"/>
            </xsl:copy>
        </xsl:if>
    </xsl:template>

    <!--CONTAINER: add label attribute that takes as its value the value of the type attribute -->
    <xsl:template match="ead:container" mode="hl-did">
        <xsl:copy>
            <xsl:copy-of select="@*"/>
            <xsl:attribute name="label">
                <xsl:value-of select="@type"/>
            </xsl:attribute>
            <xsl:apply-templates/>
        </xsl:copy>
    </xsl:template>

    <!-- EMPH: add render@ -->
    <xsl:template match="ead:emph[not(@render)]" mode="hl-did">
        <xsl:copy>
            <xsl:copy-of select="@*"/>
            <xsl:attribute name="render">italic</xsl:attribute>
            <xsl:apply-templates/>
        </xsl:copy>
    </xsl:template>
    
    <xsl:template match="ead:unitdate[not(.='undated')]" mode="hl-did">
        <xsl:copy>
            <xsl:copy-of select="@*[not(name()='calendar') and not(name()='era')]"/>

            <xsl:analyze-string select="string(.)"
                regex="(^)(\d{{4}})(\s)(January|February|March|April|May|June|July|August|September|October|November|December)(\s)(\d{{1,2}})(\s*\.*\s*$)"
                flags="xi">
                <xsl:matching-substring>
                    <xsl:if test="matches(regex-group(4), '^January$', 'i')">
                        <xsl:attribute name="normal">
                            <xsl:value-of select="regex-group(2)"/>-01-<xsl:choose>
                                <xsl:when test="matches(regex-group(6),'^\d{1}$')">0<xsl:value-of
                                        select="regex-group(6)"/>
                                </xsl:when>
                                <xsl:otherwise><xsl:value-of select="regex-group(6)"/>
                                </xsl:otherwise>
                            </xsl:choose>
                        </xsl:attribute>
                    </xsl:if>
                    <xsl:if test="matches(regex-group(4), '^February$', 'i')">
                        <xsl:attribute name="normal">
                            <xsl:value-of select="regex-group(2)"/>-02-<xsl:choose>
                                <xsl:when test="matches(regex-group(6),'^\d{1}$')">0<xsl:value-of
                                        select="regex-group(6)"/>
                                </xsl:when>
                                <xsl:otherwise><xsl:value-of select="regex-group(6)"/>
                                </xsl:otherwise>
                            </xsl:choose>
                        </xsl:attribute>
                    </xsl:if>
                    <xsl:if test="matches(regex-group(4), '^March$', 'i')">
                        <xsl:attribute name="normal">
                            <xsl:value-of select="regex-group(2)"/>-03-<xsl:choose>
                                <xsl:when test="matches(regex-group(6),'^\d{1}$')">0<xsl:value-of
                                        select="regex-group(6)"/>
                                </xsl:when>
                                <xsl:otherwise><xsl:value-of select="regex-group(6)"/>
                                </xsl:otherwise>
                            </xsl:choose>
                        </xsl:attribute>
                    </xsl:if>
                    <xsl:if test="matches(regex-group(4), '^April$', 'i')">
                        <xsl:attribute name="normal">
                            <xsl:value-of select="regex-group(2)"/>-04-<xsl:choose>
                                <xsl:when test="matches(regex-group(6),'^\d{1}$')">0<xsl:value-of
                                        select="regex-group(6)"/>
                                </xsl:when>
                                <xsl:otherwise><xsl:value-of select="regex-group(6)"/>
                                </xsl:otherwise>
                            </xsl:choose>
                        </xsl:attribute>
                    </xsl:if>
                    <xsl:if test="matches(regex-group(4), '^May$', 'i')">
                        <xsl:attribute name="normal">
                            <xsl:value-of select="regex-group(2)"/>-05-<xsl:choose>
                                <xsl:when test="matches(regex-group(6),'^\d{1}$')">0<xsl:value-of
                                        select="regex-group(6)"/>
                                </xsl:when>
                                <xsl:otherwise><xsl:value-of select="regex-group(6)"/>
                                </xsl:otherwise>
                            </xsl:choose>
                        </xsl:attribute>
                    </xsl:if>
                    <xsl:if test="matches(regex-group(4), '^June$', 'i')">
