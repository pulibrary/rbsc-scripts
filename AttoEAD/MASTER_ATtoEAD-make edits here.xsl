            <!-- Normalizes (before|after)* YYYY -->

            <xsl:analyze-string select="string(.)"
                regex="(^circa\s)(before\s*|after\s*)*(\d{{4}})(\s*\.*\s*$)" flags="xi">
                <xsl:matching-substring>
                    <xsl:attribute name="normal">
                        <xsl:value-of select="regex-group(3)"/>/<xsl:value-of
                            select="regex-group(3)"/>
                    </xsl:attribute>
                </xsl:matching-substring>
            </xsl:analyze-string>

            <!-- Normalizes YYYY-YYYY -->

            <xsl:analyze-string select="string(.)"
                regex="(^circa\s)(\d{{4}})(\s*-\s*)(\d{{4}})(\s*\.*\s*$)" flags="xi">
                <xsl:matching-substring>
                    <xsl:if test="$unitdate-node[not(@type='bulk')]">
                        <xsl:attribute name="type">inclusive</xsl:attribute>
                    </xsl:if>
                    <xsl:attribute name="normal">
                        <xsl:value-of select="regex-group(2)"/>/<xsl:value-of
                            select="regex-group(4)"/>
                    </xsl:attribute>
                </xsl:matching-substring>
            </xsl:analyze-string>

            <!-- Normalizes (before|after) YYYYs -->

            <xsl:analyze-string select="string(.)"
                regex="(^circa\s)(before\s*|after\s*)*(\d{{3}})(0)(s)(\s*\.*\s*$)" flags="xi">
                <xsl:matching-substring>
                    <xsl:if test="$unitdate-node[not(@type='bulk')]">
                        <xsl:attribute name="type">inclusive</xsl:attribute>
                    </xsl:if>
                    <xsl:attribute name="normal">
                        <xsl:value-of select="regex-group(3)"/><xsl:value-of select="regex-group(4)"
                            />/<xsl:value-of select="regex-group(3)"/>9</xsl:attribute>
                </xsl:matching-substring>
            </xsl:analyze-string>

            <!-- Normalizes YYYYs-YYYYs -->

            <xsl:analyze-string select="string(.)"
                regex="(^circa\s)(\d{{3}})(0)(s)(\s*-\s*)(\d{{3}})(0)(s)(\s*\.*\s*$)" flags="xi">
                <xsl:matching-substring>
                    <xsl:if test="$unitdate-node[not(@type='bulk')]">
                        <xsl:attribute name="type">inclusive</xsl:attribute>
