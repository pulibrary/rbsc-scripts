                                </xsl:choose></xsl:if>
                            <xsl:if test="matches(regex-group(6), '^July$', 'i')">-07-<xsl:choose>
                                    <xsl:when test="matches(regex-group(8),'^\d{1}$')"
                                            >0<xsl:value-of select="regex-group(8)"/>
                                    </xsl:when>
                                    <xsl:otherwise><xsl:value-of select="regex-group(8)"/>
                                    </xsl:otherwise>
                                </xsl:choose></xsl:if>
                            <xsl:if test="matches(regex-group(6), '^August$', 'i')">-08-<xsl:choose>
                                    <xsl:when test="matches(regex-group(8),'^\d{1}$')"
                                            >0<xsl:value-of select="regex-group(8)"/>
                                    </xsl:when>
                                    <xsl:otherwise><xsl:value-of select="regex-group(8)"/>
                                    </xsl:otherwise>
                                </xsl:choose></xsl:if>
                            <xsl:if test="matches(regex-group(6), '^September$', 'i')">-09-<xsl:choose>
                                    <xsl:when test="matches(regex-group(8),'^\d{1}$')"
                                            >0<xsl:value-of select="regex-group(8)"/>
                                    </xsl:when>
                                    <xsl:otherwise><xsl:value-of select="regex-group(8)"/>
                                    </xsl:otherwise>
                                </xsl:choose></xsl:if>
                            <xsl:if test="matches(regex-group(6), '^October$', 'i')">-10-<xsl:choose>
                                    <xsl:when test="matches(regex-group(8),'^\d{1}$')"
                                            >0<xsl:value-of select="regex-group(8)"/>
                                    </xsl:when>
                                    <xsl:otherwise><xsl:value-of select="regex-group(8)"/>
