                    <xsl:attribute name="normal">
                        <xsl:value-of select="regex-group(2)"/>/<xsl:value-of
                            select="regex-group(4)"/>9</xsl:attribute>
                </xsl:matching-substring>
            </xsl:analyze-string>

            <!-- Normalizes YYYYs-YYYY -->

            <xsl:analyze-string select="string(.)"
                regex="(^circa\s)(\d{{3}})(0)(s)(\s*-\s*)(\d{{4}})(\s*\.*\s*$)" flags="xi">
                <xsl:matching-substring>
                    <xsl:if test="$unitdate-node[not(@type='bulk')]">
                        <xsl:attribute name="type">inclusive</xsl:attribute>
                    </xsl:if>
                    <xsl:attribute name="normal">
                        <xsl:value-of select="regex-group(2)"/><xsl:value-of select="regex-group(3)"
                            />/<xsl:value-of select="regex-group(6)"/></xsl:attribute>
                </xsl:matching-substring>
            </xsl:analyze-string>

            <!-- Normalizes YYYY-(present)* -->

            <xsl:analyze-string select="string(.)"
                regex="(^circa\s)(\d{{4}})(s)*(\s*-\s*)(present|Present)*(\s*\.*\s*$)" flags="xi">
                <xsl:matching-substring>
                    <xsl:if test="$unitdate-node[not(@type='bulk')]">
                        <xsl:attribute name="type">inclusive</xsl:attribute>
                    </xsl:if>
                    <xsl:attribute name="normal">
                        <xsl:value-of select="regex-group(2)"/>/2999</xsl:attribute>
                </xsl:matching-substring>
            </xsl:analyze-string>
            <xsl:value-of select="replace(., '^bulk,*\s*', '', 'i')"></xsl:value-of>
        </xsl:copy>
    </xsl:template>

</xsl:stylesheet>
