                                    <xsl:otherwise><xsl:value-of select="regex-group(8)"/>
                                    </xsl:otherwise>
                                </xsl:choose></xsl:if>
                            <xsl:if test="matches(regex-group(6), '^December$', 'i')">-12-<xsl:choose>
                                    <xsl:when test="matches(regex-group(8),'^\d{1}$')"
                                            >0<xsl:value-of select="regex-group(8)"/>
                                    </xsl:when>
                                    <xsl:otherwise><xsl:value-of select="regex-group(8)"/>
                                    </xsl:otherwise>
                                </xsl:choose></xsl:if>
                        </xsl:attribute>
                    </xsl:if>
                    <xsl:if test="matches(regex-group(4), '^December$', 'i')">
                        <xsl:attribute name="normal">
                            <xsl:value-of select="regex-group(2)"/>-12/<xsl:value-of
                                select="regex-group(2)"/>
                            <xsl:if test="matches(regex-group(6), '^January$', 'i')">-01-<xsl:choose>
                                    <xsl:when test="matches(regex-group(8),'^\d{1}$')"
                                            >0<xsl:value-of select="regex-group(8)"/>
                                    </xsl:when>
                                    <xsl:otherwise><xsl:value-of select="regex-group(8)"/>
                                    </xsl:otherwise>
                                </xsl:choose></xsl:if>
                            <xsl:if test="matches(regex-group(6), '^February$', 'i')">-02-<xsl:choose>
                                    <xsl:when test="matches(regex-group(8),'^\d{1}$')"
                                            >0<xsl:value-of select="regex-group(8)"/>
                                    </xsl:when>
                                    <xsl:otherwise><xsl:value-of select="regex-group(8)"/>
                                    </xsl:otherwise>
                                </xsl:choose></xsl:if>
                            <xsl:if test="matches(regex-group(6), '^March$', 'i')">-03-<xsl:choose>
                                    <xsl:when test="matches(regex-group(8),'^\d{1}$')"
                                            >0<xsl:value-of select="regex-group(8)"/>
                                    </xsl:when>
                                    <xsl:otherwise><xsl:value-of select="regex-group(8)"/>
                                    </xsl:otherwise>
                                </xsl:choose></xsl:if>
                            <xsl:if test="matches(regex-group(6), '^April$', 'i')">-04-<xsl:choose>
                                    <xsl:when test="matches(regex-group(8),'^\d{1}$')"
                                            >0<xsl:value-of select="regex-group(8)"/>
